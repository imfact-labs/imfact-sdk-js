{
  "name": "@imfact/account-abstraction",
  "version": "1.0.0",
  "description": "framework for mitum network",
  "main": "dist/bundle.cjs.cjs",
  "module": "dist/bundle.esm.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "check:orphans": "npx madge --orphans ./src/**/*",
    "check:circular": "npx madge --circular --image ./circular.svg ./src/**/*",
    "check:dependency": "npx madge --dot ./src/**/* > ./dot.gv "
  },
  "exports": {
    "require": "./dist/bundle.cjs.cjs",
    "import": "./dist/bundle.esm.mjs"
  },
  "keywords": [
    "mitum",
    "mitum-account-abstraction"
  ],
  "author": "socialinfratech",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imfact-labs/imfact-sdk-js.git#account-abstraction"
  },
  "bugs": {
    "url": "https://github.com/imfact-labs/imfact-sdk-js/issues"
  },
  "homepage": "https://github.com/imfact-labs/imfact-sdk-js/tree/account-abstraction#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/globals": "^29.7.0",
    "@tsconfig/recommended": "^1.0.3",
    "@types/elliptic": "^6.4.16",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "jest": "^29.7.0",
    "madge": "^6.1.0",
    "rollup": "^4.9.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.2",
    "@noble/secp256k1": "^1.7.1",
    "axios": "^1.7.7",
    "big-integer": "^1.6.51",
    "bs58": "^5.0.0",
    "bs58check": "^3.0.1",
    "elliptic": "^6.5.4",
    "ethers": "^6.7.0",
    "int64-buffer": "^1.0.1",
    "js-sha3": "^0.8.0"
  }
}
